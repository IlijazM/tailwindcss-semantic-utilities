{
  "name": "@ilijazm/tailwindcss-section",
  "version": "1.0.1",
  "description": "Tailwind CSS Section is a plugin for Tailwind CSS v4 that adds semantic utility classes for sections which add block padding and a relative position.",
  "main": "src/tailwindcss-section.css",
  "type": "module",
  "files": [
    "src/",
    "docs/",
    "README.md"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "plugin",
    "css",
    "utilities",
    "components",
    "spacing"
  ],
  "exports": {
    ".": {
      "import": "./src/tailwindcss-section.css",
      "require": "./src/tailwindcss-section.css",
      "default": "./src/tailwindcss-section.css"
    }
  },
  "author": "Ilijaz Mehmedovic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:IlijazM/tailwindcss-semantic-utilities.git",
    "directory": "tailwindcss-section"
  },
  "dependencies": {
    "@ilijazm/tailwindcss-semantic-spacing": "^1.0.3",
    "tailwindcss": "^4.1.10"
  }
}
